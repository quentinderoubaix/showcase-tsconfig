{
	"name": "@qderoubaix/styled",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run package",
		"preview": "vite preview",
		"package": "vite build && svelte-kit sync && svelte-package && publint",
		"package-we-would-like": "vite build && svelte-kit sync && svelte-package --tsconfig tsconfig.build.json && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.build.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.build.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"dependencies": {
		"@amadeus-it-group/tansu": "^1.0.0",
		"@qderoubaix/daisy": "0.0.0",
		"svelte": "^4.2.7"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
